{
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-comment": 0
    },
    "plugins": [ "@typescript-eslint", "jsx-a11y"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": false,
      "node": true
    },
    "settings": {

    }
  }