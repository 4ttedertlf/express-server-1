{
  "name": "typescript-express",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:info": "following scripts are for local development",
    "dev:format": "prettier --write \"**/*.ts\"",
    "dev:lint": "eslint . --ext 'ts,json'",
    "dev:build": "tsc",
    "dev:start": "cross-env-shell DEBUG=express:* ts-node-dev -r dotenv/config ./src/index.ts dotenv_config_path=./.env dotenv_config_debug=true",
    "dev:test": "jest --setupFiles dotenv/config --runInBand --detectOpenHandles --testTimeout=20000 --forceExit --verbose --coverage",
    "dev:test:full": "npm run dev:format && npm run dev:lint && npm run dev:build && npm run dev:test",
    "cicd:info": "following scripts are for CICD actions/pipelines",
    "cicd:lint": "npm run dev:lint",
    "cicd:build": "tsc",
    "cicd:test": "npm run actions:test:coverage-report",
    "production:info": "following scripts are for Azure App Service",
    "build": "echo 'no build during App Service zip deploy'",
    "start": "node index.js",
    "actions:test:coverage-report": "jest --runInBand --detectOpenHandles --testTimeout=20000 --forceExit --verbose  --json --outputFile=./jest-log.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/identity": "^2.0.5",
    "@azure/keyvault-secrets": "^4.4.0",
    "@types/jest": "^28.1.1",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "jest": "^28.1.1",
    "mongodb": "^4.7.0",
    "npm": "^8.13.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.6.2",
    "ts-node": "10.6.0",
    "typescript": "^4.1.3"
  }
}
